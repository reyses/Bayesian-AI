{
  "project": "Bayesian-AI",
  "pipeline": {
    "LOAD": [
      "config/settings.py",
      "training/databento_loader.py",
      "training/orchestrator.py"
    ],
    "TRANSFORM": [
      "core/data_aggregator.py",
      "core/layer_engine.py",
      "cuda_modules/pattern_detector.py",
      "cuda_modules/confirmation.py",
      "cuda_modules/velocity_gate.py"
    ],
    "ANALYZE": [
      "core/state_vector.py",
      "core/bayesian_brain.py",
      "execution/wave_rider.py"
    ],
    "VISUALIZE": [
      "core/quantum_field_engine.py"
    ],
    "MAINTENANCE": [
      "archive/old_scripts/generate_status_report.py",
      "archive/old_scripts/manifest_integrity_check.py",
      "archive/old_scripts/verify_environment.py",
      "archive/old_scripts/build_executable.py"
    ]
  },
  "layers": {
    "L1_90d": "core/layer_engine.py",
    "L2_30d": "core/layer_engine.py",
    "L3_1wk": "core/layer_engine.py",
    "L4_daily": "core/layer_engine.py",
    "L5_4hr": "core/layer_engine.py",
    "L6_1hr": "core/layer_engine.py",
    "L7_15m": "cuda_modules/pattern_detector.py",
    "L8_5m": "cuda_modules/confirmation.py",
    "L9_1s": "cuda_modules/velocity_gate.py"
  },
  "resources": {
    "DATA_RAW": "DATA/RAW"
  }
}
