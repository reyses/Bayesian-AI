{
  "project": "ProjectX v2.0",
  "config": {
    "settings": "config/settings.py"
  },
  "resources": {
    "raw_data": "DATA/RAW"
  },
  "pipeline": {
    "LOAD": [
      "training/databento_loader.py",
      "training/orchestrator.py"
    ],
    "TRANSFORM": [
      "core/data_aggregator.py",
      "core/layer_engine.py",
      "cuda/pattern_detector.py",
      "cuda/confirmation.py",
      "cuda/velocity_gate.py"
    ],
    "ANALYZE": [
      "core/state_vector.py",
      "core/bayesian_brain.py",
      "execution/wave_rider.py"
    ],
    "VISUALIZE": [
      "engine_core.py"
    ]
  },
  "layers": {
    "L1_90d": "core/layer_engine.py",
    "L2_30d": "core/layer_engine.py",
    "L3_1wk": "core/layer_engine.py",
    "L4_daily": "core/layer_engine.py",
    "L5_4hr": "core/layer_engine.py",
    "L6_1hr": "core/layer_engine.py",
    "L7_15m": "cuda/pattern_detector.py",
    "L8_5m": "cuda/confirmation.py",
    "L9_1s": "cuda/velocity_gate.py"
  }
}
